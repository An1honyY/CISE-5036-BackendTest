{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@now/node"
    },
    {
      "src": "api/articles.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/moderate.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/api/articles",
      "dest": "api/articles.js"
    },
    {
      "src": "/api/moderate",
      "dest": "api/moderate.js"
    }
  ]
}